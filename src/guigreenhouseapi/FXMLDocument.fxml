<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="guigreenhouseapi.FXMLDocumentController">
    <children>
      <Pane id="GreenhouseData" fx:id="GreenhouseData" layoutX="511.0" layoutY="14.0" prefHeight="575.0" prefWidth="275.0">
         <children>
            <Label layoutX="14.0" layoutY="48.0" text="Temp indside" />
            <TextField fx:id="Temp_indside" editable="false" layoutX="100.0" layoutY="43.0" prefHeight="25.0" prefWidth="91.0" />
            <TextField fx:id="Temp_outside" editable="false" layoutX="100.0" layoutY="88.0" prefHeight="25.0" prefWidth="91.0" />
            <Label layoutX="12.0" layoutY="93.0" text="Temp outside" />
            <Label layoutX="12.0" layoutY="137.0" text="Level of moist" />
            <TextField fx:id="Level_of_moist" editable="false" layoutX="100.0" layoutY="133.0" prefHeight="25.0" prefWidth="91.0" />
            <TextField fx:id="Water_level" editable="false" layoutX="100.0" layoutY="178.0" prefHeight="25.0" prefWidth="91.0" />
            <Label layoutX="12.0" layoutY="182.0" text="Water level" />
            <Label layoutX="12.0" layoutY="226.0" text="Hight of plants" />
            <TextField fx:id="Hight_of_plants" editable="false" layoutX="100.0" layoutY="222.0" prefHeight="25.0" prefWidth="91.0" />
              <Button fx:id="refreshButton" layoutX="219.0" layoutY="2.0" onAction="#handleButtonAction" text="Refresh" />
         </children>
      </Pane>
      <Pane layoutX="14.0" layoutY="46.0" prefHeight="543.0" prefWidth="275.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Temp" />
            <Label layoutX="15.0" layoutY="51.0" text="Red light" />
            <Spinner fx:id="tempSpinner" initialValue="25" layoutX="112.0" layoutY="10.0" max="30" min="0" />
            <Spinner fx:id="redLightSpinner" initialValue="92" layoutX="112.0" layoutY="47.0" max="100" min="0" />
            <Label layoutX="15.0" layoutY="88.0" text="Blue light" />
            <Spinner fx:id="blueLightSpinner" initialValue="8" layoutX="112.0" layoutY="84.0" max="100" min="0" />
            <Label layoutX="15.0" layoutY="126.0" text="Level of moist" />
            <Spinner fx:id="levelOfMoistSpinner" initialValue="55" layoutX="112.0" layoutY="122.0" max="100" min="0" />
            <Label layoutX="15.0" layoutY="163.0" text="Add water" />
            <Button fx:id="addWaterButton" layoutX="112.0" layoutY="159.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="10 sek" />
            <Button fx:id="setValuesButton" layoutX="112.0" layoutY="211.0" mnemonicParsing="false" onAction="#setValues" text="Set  values" />
         </children>
      </Pane>
    </children>
</AnchorPane>
